{
    "schema": "schema.graphql",
    "documents": "**/*.graphql",
    "extensions": {
        "strawberryShake": {
            "name": "GraphQlClient",
            "dependencyInjection": true,
            "accessModifier": "public",
            "records": {
                "inputs": false,
                "entities": false
            },
            "transportProfiles": [
                {
                    "default": "Http"
                }
            ]
        }
    }
}